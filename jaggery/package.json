{
  "name": "jaggery",
  "displayName": "jaggery-syntax",
  "description": "Highlighting for jaggery syntax in .jag files",
  "icon": "images/jaggeryjs.png",
  "version": "0.0.1",
  "publisher": "STBaranovAN",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "update-grammar": "node ../../build/npm/update-grammar.js textmate/html.tmbundle Syntaxes/HTML.plist ./syntaxes/html.tmLanguage.json"
  },
  "contributes": {
    "languages": [
      {
        "id": "jaggery",
        "extensions": [
          ".html",
          ".htm",
          ".shtml",
          ".xhtml",
          ".mdoc",
          ".jsp",
          ".asp",
          ".aspx",
          ".jshtm",
          ".volt",
          ".ejs",
          ".rhtml",
          ".jag"
        ],
        "aliases": [
          "HTML",
          "htm",
          "html",
          "xhtml"
        ],
        "mimetypes": [
          "text/html",
          "text/x-jshtm",
          "text/template",
          "text/ng-template",
          "application/xhtml+xml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jaggery",
        "scopeName": "jaggery.html",
        "path": "./syntaxes/html.tmLanguage.json",
        "embeddedLanguages": {
          "text.html": "html",
          "source.css": "css",
          "source.js": "javascript",
          "source.python": "python",
          "source.smarty": "smarty",
          "section.embedded.source.cshtml": "csharp"
        },
        "tokenTypes": {
          "meta.tag string.quoted": "other"
        }
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/html.snippets.json"
      }
    ]
  }
}